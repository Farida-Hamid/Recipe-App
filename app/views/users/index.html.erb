<ul class="user_container--list">
  <% @users.each do |user| %>
    <li class="user_list--link">
      <p class="user-name"><%= user.name %></p>
      <div class="association">
        Recipes: <%= user.recipe_counter %>,
        Foods: <%= user.food_counter %>
      </div>
    </li>
  <% end %>
</ul>